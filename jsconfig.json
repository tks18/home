{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@a/*": ["src/assets/*"],
      "@c/*": ["src/components/*"],
      "@l/*": ["src/libs/*"],
      "@p/*": ["src/plugins/*"],
      "@r/*": ["src/router/*"],
      "@styles/*": ["src/styles/*"],
      "@t/*": ["src/templates/*"],
      "@v/*": ["src/views/*"],
      "@state": ["src/plugins/state/index.js"]
    }
  },
  "typeAcquisition": {
    "exclude": [
      "agent-base",
      "dotenv",
      "https-proxy-agent",
      "socks-proxy-agent"
    ]
  },
  "exclude": ["node_modules", "dist"],
  "include": ["src/**/*"]
}
